{
    "name": "bytesfield/simple-kyc",
    "description": "A PHP package for Know Your Customer (KYC)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Simple",
        "Kyc",
        "Customer"
    ],
    "homapage": "https://github.com/bytesfield/simple-kyc",
    "authors": [
        {
            "name": "Abraham Udele",
            "email": "abrahamudele@gmail.com",
            "homepage": "https://abrahamudele.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bytesfield\\SimpleKyc\\": "src/"
        },
        "files": [
            "src/classes/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Bytesfield\\SimpleKyc\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0.1",
        "mpociot/pipeline": "9999999-dev",
        "hassankhan/config": "9999999-dev"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7"
    }
}
